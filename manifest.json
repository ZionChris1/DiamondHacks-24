{
    "manifest_version": 3,
    "name": "Diamond Hacks Phishing",
    "version": "1.0",
    "description": "Detects fishy urls and redirects the user back to the legit website",
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "512": "images/icon512.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "webRequest"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js", "background.js"],
            "css": ["style.css"]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "512": "images/icon512.png"
        }
    }
}