{
    "manifest_version": 3,
    "name": "phish-net",
    "version": "1.0",
    "description": "Detects fishy urls and automatically blocks them.",
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "128": "images/icon128.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "webRequest"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "128": "images/icon128.png"
        }
    }
}